@model string

<div class="jumbotron">
    <h1>F# + ASP.NET</h1>
    <div class="lead">
        @if (string.IsNullOrEmpty(Model))
        {
        <form method="post">
            Your name: <input type="text" name="name" /><input type="submit" value="OK"/>
        </form>
        } else
        {
            <span>Hello @Model</span>
        }
    </div>
</div>
